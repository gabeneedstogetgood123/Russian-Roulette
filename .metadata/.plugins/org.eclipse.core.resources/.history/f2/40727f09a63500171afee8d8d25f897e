import java.util.*;
public class weapons {
	private static Random generator = new Random();
	private String [] revolver =new String[6];
	public String[] getRevolver(){
		return revolver;
	}
	public static int reload(){
		return generator.nextInt(6);
		}

}	

	for(int i=0;i<5;i++){
		revolver[i]="You are safe.";
	}
		

}